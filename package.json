{
  "name": "storinator",
  "version": "0.1.0",
  "description": "A helper library for local storage.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "umd:main": "dist/umd/index.js",
  "types": "dist/types/index.d.js",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:umd && npm run build:types",
    "build:cjs": "node tools/cleanup cjs && tsc -p config/tsconfig.cjs.json",
    "build:esm": "node tools/cleanup esm && tsc -p config/tsconfig.esm.json",
    "build:umd": "node tools/cleanup umd && webpack --config config/webpack.config.js",
    "build:types": "node tools/cleanup types && tsc -p config/tsconfig.types.json",
    "test": "mocha -require mock-local-storage 'test/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mircoxi/storinator.git"
  },
  "keywords": [],
  "author": "Mircoxi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mircoxi/storinator/issues"
  },
  "homepage": "https://github.com/Mircoxi/storinator#readme",
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/mocha": "^9.1.1",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "mock-local-storage": "^1.1.23",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ]
}
